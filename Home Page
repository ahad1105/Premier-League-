package seleniumproj;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.support.ui.Select;

public class Page1 {

	public static void main(String[] args) throws InterruptedException {
		WebDriver driver=new ChromeDriver();
		
		//load the Website
		driver.get("https://www.premierleague.com/matchweek/7866/blog");
		
		//Maximize current window
		driver.manage().window().maximize();
		
		Thread.sleep(5000);
		
		//click on Accept cookies button
		driver.findElement(By.xpath("/html/body/div[2]/div[2]/div/div/div[2]/div/div/button")).click();
		
		Thread.sleep(5000);
		
		//click on sign in field
		driver.findElement(By.xpath("/html/body/header/div/div/section/a[1]/span")).click();
		Thread.sleep(5000);
    
    }}
